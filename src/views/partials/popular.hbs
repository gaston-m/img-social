<div class="card mb-2">
    <div class="card-header bg-dark text-white">
        <h4><i class="fas fa-fire"> Mas Popular: </i></h4>
    </div>
    <div class="card-body">
        <div class="row">
        {{#each sidebar.popular}}

            <div class="col-md-4">
                <a href="/images/{{uniqueId}}">
                    <img src="/upload/{{filename}}" alt="The Best image" class="w-100 h-100 image-thumbnail">
                </a>
            </div>            
        {{/each}}

        </div>
    </div>
</div>